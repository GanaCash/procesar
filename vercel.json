{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/postback",
            "dest": "server.js"
        },
        {
            "src": "/get-points",
            "dest": "server.js"
        },
        {
            "src": "/(.*)",
            "dest": "public/$1"
        }
    ]
}
